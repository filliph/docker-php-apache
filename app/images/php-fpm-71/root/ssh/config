Host *
  AddKeysToAgent yes
  
Host github.com
  HostName github.com
  #Port 443
  PreferredAuthentications publickey
  AddKeysToAgent yes
  IdentityFile /root/.ssh/github

Host ssh.github.com
  HostName ssh.github.com
  #Port 443
  PreferredAuthentications publickey
  AddKeysToAgent yes
  IdentityFile /root/.ssh/github